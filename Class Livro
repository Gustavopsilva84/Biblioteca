package trabalho;

public class Livro {

    private String nome, autor, editora, isbn;
    private boolean livroAlugado;

    public String getNome() {
	return nome;
    }

    public String getAutor() {
	return autor;
    }

    public String getEditora() {
	return editora;
    }

    public String getIsbn() {
	return this.isbn;
    }

    public boolean getLivroAlugado() {
	return livroAlugado;
    }

    public void setLivroAlugado(boolean livroAlugado) {
	this.livroAlugado = livroAlugado;
    }

    public Livro() {
	this.nome = null;
	this.autor = null;
	this.editora = null;
	this.isbn = null;
	setLivroAlugado(false);
    }

    public Livro(String nome, String autor, String editora, String isbn) {
	this.nome = nome;
	this.autor = autor;
	this.editora = editora;
	this.isbn = isbn;
	setLivroAlugado(false);
    }
    
    
}
